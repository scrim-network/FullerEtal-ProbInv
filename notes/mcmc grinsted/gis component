source('gringis.R')
grgisConfigAssim(obserr=F)
grgisRunAssim(nbatch=10000,initial=T)
grgisRunSeqPredict(nbatch=10000)
prqplotsub(
    prgrgisctx$seq_gischain,
    legends=c("mean GIS", "95% CI GIS"),
    col=c("blue", "red"),
    lty=c("solid", "dashed"),
    prctx=prgrgisctx,
    obs=F,
    shade=F,
    times=as.numeric(colnames(prgrgisctx$prchain)),
