load("~/runs/grgis_split/predict")
source('predict.R')
newDev("ppred", outfile=T)
par(mfcol=c(2, 2), omi=c(0.5, 0, 0, 0))
prqplotsub(
    prgrgisctx$gischain,
    legends=c("mean GIS", "95% CI GIS"),
    col=c("blue", "blue", "red", "red"),
    lty=c("solid", "dashed", "solid", "dotted"),
    prctx=prgrgisctx,
    obs=F,
    shade=F,
    endYear=2001,
    newdev=F,
    caption=""
    )
title(sub="Hindcast of GIS")
prqplotsub(
    prgrgisctx$otherchain,
    legends=c("mean other", "95% CI other"),
    col=c("blue", "blue", "red", "red"),
    lty=c("solid", "dashed", "solid", "dotted"),
    prctx=prgrgisctx,
    obs=F,
    shade=F,
    endYear=2002,
    newdev=F,
    caption=""
    )
title(sub="Hindcast of Other")
prqplotsub(
    prgrgisctx$gischain,
    legends=c("mean GIS", "95% CI GIS"),
    col=c("blue", "blue", "red", "red"),
    lty=c("solid", "dashed", "solid", "dotted"),
    prctx=prgrgisctx,
    obs=F,
    shade=F,
    startYear=2002,
    newdev=F,
    caption=""
    )
title(sub="Forecast of GIS")
