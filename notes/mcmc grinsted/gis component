load("~/runs/grgis_split/predict")
source("gringis.R")
grgisRunSeqPredict(nbatch=100000)
prgrgisctx$prchain <- prgrgisctx$seq_gischain
source('predict.R')
prqplotsub(
    prgrgisctx$seq_gischain,
    prgrgisctx$seq_otherchain,
    legends=c("mean GIS", "95% CI GIS", "mean other", "95% CI other"),
    col=c("blue", "blue", "red", "red"),
    lty=c("solid", "dashed", "solid", "dotted"),
    prctx=prgrgisctx,
    obs=F,
    shade=F,
    times=as.numeric(colnames(prgrgisctx$prchain)),
    xcaption="Temperature (Celsius)",
    ycaption="Equilibrium Sea Level Anomaly (m)",
    caption="Figure 1. Posterior Predictive for Equilibrium Sea Level",
    newdev=T,
    outfiles=T
    )

prqplotsub(
    prgrgisctx$gischain,
    prgrgisctx$otherchain,
    legends=c("mean GIS", "95% CI GIS", "mean other", "95% CI other"),
    col=c("blue", "blue", "red", "red"),
    lty=c("solid", "dashed", "solid", "dotted"),
    prctx=prgrgisctx,
    obs=F,
    shade=F,
    startYear=2002,
    caption="Fig 1. Posterior Predictive for Global Mean Sea Level by Component",
    outfiles=T
    )
prqplotsub(
    prgrgisctx$gischain,
    prgrgisctx$otherchain,
    legends=c("mean GIS", "95% CI GIS", "mean other", "95% CI other"),
    col=c("blue", "blue", "red", "red"),
    lty=c("solid", "dashed", "solid", "dotted"),
    prctx=prgrgisctx,
    obs=F,
    shade=F,
    endYear=2001,
    caption="Fig 1. Posterior Predictive for Global Mean Sea Level by Component",
    outfiles=T
    )





